        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.pontual.sgmr.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": ":?query:",
          "target": [
            "TargetApp"
          ]
        },
        {
          "name": "Administrativo",
          "pattern": "Administrativo",
          "target": [
            "Administrativo"
          ]
        },
        {
          "name": "AssociarFormulario",
