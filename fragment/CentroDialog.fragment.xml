<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>centro}"
		rememberSelections="false"
		items="{listaCentrosModel>/}"
		search="_handleValueHelpSearchCentro"
		confirm="_handleValueHelpCloseCentro"
		cancel=""
		showClearButton="true"		
		multiSelect="true">
		<StandardListItem
			icon=""		
			iconDensityAware="false"
			iconInset="false"
			title="{listaCentrosModel>Werks}" 
			description="{listaCentrosModel>Name1}" 
			selected="{listaCentrosModel>Selected}"
			/>
	</SelectDialog>
</core:FragmentDefinition>