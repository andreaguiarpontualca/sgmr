<mvc:View
    controllerName="com.pontual.sgmr.controller.Cabecalho"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
>
    <VBox
        id="idVBoxCabecalho"
        class="sapUiSmallMargin"
    >
        <f:Form
            id="idCabecalhoForm"
            editable="true"
        >
            <f:layout>
                <f:ResponsiveGridLayout
                    id="idResponsiveGridLayout_R220LC-9"
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="12"
                    labelSpanS="12"
                    adjustLabelSpan="false"
                    emptySpanXL="0"
                    emptySpanL="0"
                    emptySpanM="0"
                    emptySpanS="0"
                    columnsXL="2"
                    columnsL="2"
                    columnsM="2"
                    singleContainerFullSize="false"
                />
            </f:layout>
            <f:formContainers>
                <f:FormContainer
                    id="idFormUltimaMedicao"
                    title="{i18n>ultimamedicao}"
                >
                    <f:formElements>
                        <f:FormElement
                            id="idFormElementOper"
                            label="{i18n>ultimamedicao}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>UltMedEqpto}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFormElementUltimaMedData"
                            label="{i18n>data}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>DtHrEqpto}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
                <f:FormContainer
                    id="idFormMedicaoAtual"
                    title="{i18n>dadosinspecao}"
                >
                    <f:formElements>
                        <f:FormElement
                            id="idFEMedAtual"
                            label="{i18n>ultimoponto}"
                        >
                            <f:fields>
                                <Input
                                    id="idInputMedEqpto"
                                    value="{materialRodanteFormularioModel>MedEquipamento}"
                                    type="Number"
                                    maxLength="18"
                                    required="true"
                                    showClearIcon="true"
                                    width="100%"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFEDataAtual"
                            label="{i18n>data}"
                        >
                            <f:fields>
                                <DateTimePicker
                                    id="idInputData"
                                    required="true"
                                    change="handleChange"
                                    class="sapUiSmallMarginBottom"
                                    width="100%"
                                    showCurrentDateButton="true"
                                    showCurrentTimeButton="true"
                                    value="{
											'path': 'materialRodanteFormularioModel>Data',
											'type': 'sap.ui.model.type.DateTime',
											'formatOptions': {
												'pattern': 'dd/MM/y HH:mm:ss' 
											}
										}"
                                />
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
                <f:FormContainer title="{i18n>esteiraesquerda}">
                    <f:formElements>
                        <f:FormElement
                            id="idFETAGE"
                            label="{i18n>tag}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>TagE}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFEMedTagEsquerda"
                            label="{i18n>ultimamedicao}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>MedEsquerda}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFEMedDataEsquerda"
                            label="{i18n>data}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>DtHrEsq}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
                <f:FormContainer title="{i18n>esteiradireita}">
                    <f:formElements>
                        <f:FormElement
                            id="idFETAGD"
                            label="{i18n>tag}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>TagD}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFEMedTagDireita"
                            label="{i18n>ultimamedicao}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>MedDireita}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement
                            id="idFEMedDataDireita"
                            label="{i18n>data}"
                        >
                            <f:fields>
                                <ObjectStatus
                                    class="sapMObjectStatusMedium"
                                    text="{materialRodanteFormularioModel>DtHrDir}"
                                    inverted="true"
                                    state="Indication05"
                                />
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
    </VBox>
</mvc:View>
