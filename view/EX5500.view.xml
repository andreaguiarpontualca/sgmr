<mvc:View 
	controllerName="com.pontual.sgmr.controller.EX5500"    
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:blockcolor="com.pontual.sgmr.view.EX5500"
	xmlns:core="sap.ui.core"
	height="100%">
	 <m:Page id="idEX5500" 
          title="{i18n>title}" 
          titleAlignment="Center"
          showNavButton="true" 
          showSubHeader="true"
          navButtonText="Sair"
          icon="sap-icon://visits"
          navButtonPress="onNavBack"         
          class="sapUiContentPadding sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--content sapUiResponsivePadding--footer">
		   <m:headerContent>
				<m:ObjectStatus
					id="IdNetworkStatus2"
					active="false"
					inverted="true"
					icon="{path: 'conexaoModel>statusConexao',
						formatter: 'com.pontual.sgmr.model.formatter.getIconeConexao'}"
					state="{path: 'conexaoModel>statusConexao',
							formatter: 'com.pontual.sgmr.model.formatter.getCorIconeConexao'}"
				/>
        	</m:headerContent>   
			<ObjectPageLayout id="ObjectPageLayout_EX5500" upperCaseAnchorBar="false">
				<headerTitle>
					<ObjectPageHeader id="IdObjectPageHeader1_EX5500" objectTitle="INSPEÇÃO E MEDIÇÃO DE MATERIAL RODANTE"
						objectSubtitle="">
					</ObjectPageHeader>
				</headerTitle>

				<headerContent>
					<m:ObjectAttribute id="idHeader1_EX5500" text="Equipamento: {materialRodanteCriarModel>Codigo}&#10; Descrição: {materialRodanteCriarModel>DescrMaterialRodante}&#10;Formulário: {materialRodanteCriarModel>CodigoFormulario}"/>
				</headerContent>

				<sections>
					<ObjectPageSection id="idObjectSectionCabecalho_EX5500" titleUppercase="false" title="Cabeçalho">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionCabecalho_EX5500" title="Cabeçalho" titleUppercase="false">
								<blocks>
									<blockcolor:Cabecalho id="cabecalho_EX5500"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<ObjectPageSection id="idObjectSectionRoleteSup_EX5500" titleUppercase="false" title="{i18n>roletesuperior}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRoleteSup_EX5500" title="{i18n>roletesuperior}" titleUppercase="false">
								<blocks>
									<blockcolor:RoletesSuperiores id="roletes_supe_EX5500"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionRoleteInf_EX5500" titleUppercase="false" title="{i18n>roleteinferior}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRoleteInf_EX5500" title="{i18n>roleteinferior}" titleUppercase="false">								
								<blocks>
									<blockcolor:RoletesInferiores id="roletes_infe_EX5500"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					
					<ObjectPageSection id="idObjectSectionSapata_EX5500" titleUppercase="false" title="{i18n>sapatas}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionSapata_EX5500" title="{i18n>sapatas}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgSapata_EX5500" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="sapatasAE" />
											<core:CustomData key="titulo" value="{i18n>sapatas}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:Sapatas id="sapatas"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>


					<ObjectPageSection id="idObjectSectionPassoEsteiras" titleUppercase="false" title="{i18n>passoesteiras}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionPassoEsteiras" title="{i18n>passoesteiras}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgPassoEsteiras" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="passoesteira4pinos" />
											<core:CustomData key="titulo" value="{i18n>passoesteiras}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:PassoEsteiras id="passo_esteiras"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionFelchaEsteiras" titleUppercase="false" title="{i18n>flechaesteiras}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionFelchaEsteiras" title="{i18n>flechaesteiras}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgFelchaEsteiras" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="flechaesteiras" />
											<core:CustomData key="titulo" value="{i18n>flechaesteiras}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:FlechaEsteiras id="flecha_esteiras"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionRodasMotrizes" titleUppercase="false" title="{i18n>rodasmotrizes}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRodasMotrizes" title="{i18n>rodasmotrizes}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgRodasMotrizes" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="rodasmotrizes" />
											<core:CustomData key="titulo" value="{i18n>rodasmotrizes}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:RodasMotrizes id="roda_motrizes"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionRodasGuias" titleUppercase="false" title="{i18n>rodasguias}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRodasGuias" title="{i18n>rodasguias}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgRodasGuias" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="rodasguias" />
											<core:CustomData key="titulo" value="{i18n>rodasguias}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:RodasGuias id="rodas_guias"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionPinosSapatas" titleUppercase="false" title="{i18n>pinossapatas}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionPinosSapatas" title="{i18n>pinossapatas}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgPinosSapatas" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="pinossapatas430" />
											<core:CustomData key="titulo" value="{i18n>pinossapatas}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:PinosSapatas id="pinossapatas"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Condição Operação">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:CondicaoOperacao id="condicaooperacao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Roletes">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Roletes id="roletes"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Inspeção">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Inspecao id="inspecao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>


					<ObjectPageSection titleUppercase="false" title="Observações">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Observacoes id="observacao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

				</sections>

			</ObjectPageLayout>
			<m:footer>
				<m:OverflowToolbar>
					<m:Button id="messagePopoverBtn"
						icon="{ path: '/', formatter: '.buttonIconFormatter' }"
						type="{ path: '/', formatter: '.buttonTypeFormatter' }"
						text= "{path: '/', formatter: '.highestSeverityMessages' }"
						press=".handleMessagePopoverPress"
						ariaHasPopup="Dialog"/>		 
					<m:ToolbarSpacer/>
					<m:Button id="confirmarComboioButton" text="{i18n>confirmar}" type="Success" press="onConfirmarComboio" icon="sap-icon://save" ></m:Button>
					<m:Button id="cancelarComboioButton" text="{i18n>cancelar}" type="Reject" press="onCancelarComboio" icon="sap-icon://cancel" ></m:Button>                
				</m:OverflowToolbar>
			</m:footer>
	</m:Page>
</mvc:View>
