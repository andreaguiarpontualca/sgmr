								</actions>
								<blocks>
									<blockcolor:Sapatas id="sapatas"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionBuchas" titleUppercase="false" title="{i18n>buchas}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionBuchas" title="{i18n>buchas}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgBuchas" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="bucha" />
											<core:CustomData key="titulo" value="{i18n>buchas}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:Buchas id="buchas"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<!-- Elos -->
					<ObjectPageSection id="idObjectSectionElos" titleUppercase="false" title="{i18n>elos}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionElos" title="{i18n>elos}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgElos" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="elos" />
											<core:CustomData key="titulo" value="{i18n>elos}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:Elos id="elos"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<!-- Elos -->
					<ObjectPageSection id="idObjectSectionRodaGuia" titleUppercase="false" title="{i18n>rodaguia}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRodaGuia" title="{i18n>rodaguia}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgRodaGuia" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="rodaguia" />
											<core:CustomData key="titulo" value="{i18n>rodaguia}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:RodaGuia id="roda_guia"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionPassoEsteira" titleUppercase="false" title="{i18n>passoesteira}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionPassoEsteira" title="{i18n>passoesteira}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgPassoEsteira" icon="sap-icon://image-viewer" press="_handleExibirImagem">
