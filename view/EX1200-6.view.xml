<mvc:View 
	controllerName="com.pontual.sgmr.controller.EX1200-6"    
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:blockcolor="com.pontual.sgmr.view.EX1200-6"
	xmlns:core="sap.ui.core"
	height="100%">
	 <m:Page id="idEX1200-6" 
          title="{i18n>title}" 
          titleAlignment="Center"
          showNavButton="true" 
          showSubHeader="true"
          navButtonText="Sair"
          icon="sap-icon://visits"
          navButtonPress="onNavBack"         
          class="sapUiContentPadding sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--content sapUiResponsivePadding--footer">
		   <m:headerContent>
				<m:ObjectStatus
					id="IdNetworkStatus"
					active="false"
					inverted="true"
					icon="{path: 'conexaoModel>statusConexao',
						formatter: 'com.pontual.sgmr.model.formatter.getIconeConexao'}"
					state="{path: 'conexaoModel>statusConexao',
							formatter: 'com.pontual.sgmr.model.formatter.getCorIconeConexao'}"
				/>
        	</m:headerContent>   
			<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">
				<headerTitle>
					<ObjectPageHeader id="IdObjectPageHeader1" objectTitle="INSPEÇÃO E MEDIÇÃO DE MATERIAL RODANTE"
						objectSubtitle="">
					</ObjectPageHeader>
				</headerTitle>

				<headerContent>
					<m:ObjectAttribute id="idHeader1" text="Equipamento: {materialRodanteCriarModel>Codigo}&#10; Descrição: {materialRodanteCriarModel>DescrMaterialRodante}&#10;Formulário: {materialRodanteCriarModel>CodigoFormulario}"/>
				</headerContent>

				<sections>
					<ObjectPageSection id="idObjectSectionCabecalho" titleUppercase="false" title="Cabeçalho">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionCabecalho" title="Cabeçalho" titleUppercase="false">
								<blocks>
									<blockcolor:Cabecalho id="cabecalho"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<ObjectPageSection id="idObjectSectionRoleteSup" titleUppercase="false" title="{i18n>roletesuperior}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRoleteSup" title="{i18n>roletesuperior}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgRoleteSup" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="roletesuperior" />
											<core:CustomData key="titulo" value="{i18n>roletesuperior}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:RoletesSuperiores id="roletes_supe"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<ObjectPageSection id="idObjectSectionRoleteInf" titleUppercase="false" title="{i18n>roleteinferior}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionRoleteInf" title="{i18n>roleteinferior}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgRoleteInf" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="roleteinferior" />
											<core:CustomData key="titulo" value="{i18n>roleteinferior}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:RoletesInferiores id="roletes_infe"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
					<ObjectPageSection id="idObjectSectionSapata" titleUppercase="false" title="{i18n>sapatas}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionSapata" title="{i18n>sapatas}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgSapata" icon="sap-icon://image-viewer" press="_handleExibirImagem">
