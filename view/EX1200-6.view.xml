					<ObjectPageSection id="idObjectSectionTensionamento" titleUppercase="false" title="{i18n>tensionamento}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionTensionamento" title="{i18n>tensionamento}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgTensionamento" icon="sap-icon://image-viewer" press="_handleExibirImagem">
