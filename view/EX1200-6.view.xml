							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection id="idObjectSectionSegmento" titleUppercase="false" title="{i18n>segmento}">
						<subSections>
							<ObjectPageSubSection id="idObjectSubSectionSegmento" title="{i18n>segmento}" titleUppercase="false">
								<actions>
									<m:Button id="idButtonImgSegmento" icon="sap-icon://image-viewer" press="_handleExibirImagem">
										<m:customData>
											<core:CustomData key="imagem" value="segmentoA" />
											<core:CustomData key="titulo" value="{i18n>segmento}" />
										</m:customData>
									</m:Button>
								</actions>
								<blocks>
									<blockcolor:Segmento id="segmento"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Condição Operação">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:CondicaoOperacao id="condicaooperacao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Roletes">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Roletes id="roletes"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

					<ObjectPageSection titleUppercase="false" title="Inspeção">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Inspecao id="inspecao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>


					<ObjectPageSection titleUppercase="false" title="Observações">
						<subSections>
							<ObjectPageSubSection titleUppercase="false">
								<blocks>
									<blockcolor:Observacoes id="observacao"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>

				</sections>

			</ObjectPageLayout>
			<m:footer>
				<m:OverflowToolbar>
					<m:Button id="messagePopoverBtn"
						icon="{ path: '/', formatter: '.buttonIconFormatter' }"
						type="{ path: '/', formatter: '.buttonTypeFormatter' }"
						text= "{path: '/', formatter: '.highestSeverityMessages' }"
						press=".handleMessagePopoverPress"
						ariaHasPopup="Dialog"/>		 
					<m:ToolbarSpacer/>
					<m:Button id="confirmarComboioButton" text="{i18n>confirmar}" type="Success" press="onConfirmarComboio" icon="sap-icon://save" ></m:Button>
					<m:Button id="cancelarComboioButton" text="{i18n>cancelar}" type="Reject" press="onCancelarComboio" icon="sap-icon://cancel" ></m:Button>                
				</m:OverflowToolbar>
			</m:footer>
	</m:Page>
</mvc:View>
